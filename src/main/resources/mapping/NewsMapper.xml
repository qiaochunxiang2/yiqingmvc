<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sk.yiqingmvc.dao.NewsMapper">

    <!--    查询所有的新闻-->
    <select id="findAll" resultType="com.sk.yiqingmvc.model.News">
        select *
        from news
        order by id desc
        limit 10
    </select>
</mapper>